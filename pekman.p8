pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
local num = 0
local move = 1
function _init()
	x = 64
	y = 64
	booster = false
end

function _update()
	if (btn(0)) then x=x-move end
	if (btn(1)) then x=x+move end
	if (btn(2)) then y=y-move end
	if (btn(3)) then y=y+move end
	num = normaliz()
	if (btn(q)) then
	--	booster = true
	end
end

function _draw()
	cls(32)
--	print("hello world", x, y, 7)
	circfill(85, 122, 20, 14)
	sspr(0,0,10,8, x, y, 32,32)
	print(num,64, 64 , 7)
	sspr(17, 2, 6, 5, 68,68,32,32)
	if booster then
		move = 5
	else
		move = 1
	end		
end

function normaliz()
	num = num+1
	return num
end

